{
   "iisSettings": {
      "windowsAuthentication": false,
      "anonymousAuthentication": true,
      "iisExpress": {
         "applicationUrl": "http://localhost:59534",
         "sslPort": 44308
      }
   },
   "$schema": "http://json.schemastore.org/launchsettings.json",
   "profiles": {
      "IIS Express": {
         "commandName": "IISExpress",
         "launchBrowser": true,
         "launchUrl": "api/crm",
         "environmentVariables": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         }
      },
      "Platform Api": {
         "commandName": "Project",
         "launchBrowser": true,
         "launchUrl": "api/crm",
         "environmentVariables": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         },
         "applicationUrl": "https://localhost:5001;http://localhost:5000"
      },
      "Docker": {
         "commandName": "Docker",
         "launchBrowser": true,
         "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/crm",
         "publishAllPorts": true,
         "useSSL": true
      }
   }
}